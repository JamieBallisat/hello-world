function fizzbuzz(n){
  var n;
for (let n=1; n<=100; n++){console.log(n)}
if ((n % 3 == 0) && (n % 5 == 0)) {return("fizzbuzz")}

if (n % 3 == 0) {return("fizz")}

if (n % 5 == 0) {return("buzz")}

return (n)
}
console.log(fizzbuzz(1))
console.log(fizzbuzz(2))
console.log(fizzbuzz(3))
